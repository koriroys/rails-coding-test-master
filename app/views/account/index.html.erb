<table class="table table-striped">
  <thead class="thead-inverse">
    <tr>
      <th>ID</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
  <% @orders.each do |order| %>
    <tr>
      <td>
        <%= order.id %>
      </td>
      <td>
        <%= order.status %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>